<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane stylesheets="@../thoth/style.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.StateWarehouseController">
   <children>
      <VBox prefHeight="761.0" prefWidth="1280.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox>
               <children>
                  <TextField fx:id="searchSWCity" prefHeight="26.0" prefWidth="353.0" promptText="Przeszukaj magazyn" styleClass="textarea" stylesheets="@../thoth/style.css" />
                  <Button fx:id="searchStateWarehouse" mnemonicParsing="false" onAction="#searchStateWarehouse" prefHeight="26.0" prefWidth="164.0" styleClass="bluebutton" stylesheets="@../thoth/style.css" text="Szukaj/Odśwież">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </HBox>
            <TableView fx:id="stateWarehouse" editable="true" prefHeight="800.0" prefWidth="1280.0">
               <columns>
                  <TableColumn fx:id="PRODUCTID" prefWidth="100.0" text="id_produkt" />
                  <TableColumn fx:id="NAME" minWidth="0.0" prefWidth="837.0" text="nazwa" />
                  <TableColumn fx:id="PRICE" minWidth="0.0" prefWidth="93.0" text="cena" />
                  <TableColumn fx:id="AMOUNT" prefWidth="137.0" styleClass="editable-column" text="ilosc" />
                  <TableColumn fx:id="DISCOUNT" prefWidth="112.0" text="znizka" />
               </columns>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </TableView>
         </children>
      </VBox>
   </children>
</AnchorPane>
