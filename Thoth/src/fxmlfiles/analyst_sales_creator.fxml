<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="800.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AnalystSalesCreatorViewController">
   <children>
      <HBox>
         <children>
            <VBox prefHeight="800.0" prefWidth="640.0">
               <children>
                  <HBox prefHeight="46.0" prefWidth="640.0">
                     <children>
                        <TextField promptText="10%" styleClass="textarea" stylesheets="@../thoth/style.css" HBox.hgrow="NEVER">
                           <HBox.margin>
                              <Insets right="10.0" />
                           </HBox.margin>
                        </TextField>
                        <Button mnemonicParsing="false" styleClass="bluebutton" stylesheets="@../thoth/style.css" text="Ustaw">
                           <HBox.margin>
                              <Insets right="10.0" />
                           </HBox.margin>
                        </Button>
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </HBox>
                  <TableView prefHeight="800.0" prefWidth="640.0">
                     <columns>
                        <TableColumn minWidth="70.0" prefWidth="70.0" text="id_produkt" />
                        <TableColumn prefWidth="390.0" text="nazwa" />
                        <TableColumn prefWidth="90.0" text="cena" />
                        <TableColumn prefWidth="90.0" text="zniżka" />
                     </columns>
                  </TableView>
               </children>
            </VBox>
            <VBox prefHeight="800.0" prefWidth="640.0">
               <children>
                  <HBox prefHeight="21.0" prefWidth="672.0">
                     <children>
                        <TextField promptText="Nazwa produktu" styleClass="textarea" stylesheets="@../thoth/style.css">
                           <HBox.margin>
                              <Insets right="10.0" />
                           </HBox.margin>
                        </TextField>
                        <Button mnemonicParsing="false" styleClass="bluebutton" stylesheets="@../thoth/style.css" text="Szukaj">
                           <HBox.margin>
                              <Insets right="10.0" />
                           </HBox.margin>
                        </Button>
                        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="26.0" prefWidth="364.0" styleClass="whitelabel" stylesheets="@../thoth/style.css" text="Produkty" textAlignment="CENTER" textFill="#da8618">
                           <HBox.margin>
                              <Insets />
                           </HBox.margin>
                           <font>
                              <Font name="System Bold" size="18.0" />
                           </font>
                        </Label>
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </HBox>
                  <TableView fx:id="productsTable" prefHeight="800.0" prefWidth="640.0">
                    <columns>
                      <TableColumn fx:id="PRODUCTID" minWidth="70.0" prefWidth="70.0" text="id_produkt" />
                      <TableColumn fx:id="NAME" prefWidth="390.0" text="nazwa" />
                        <TableColumn fx:id="PRICE" prefWidth="90.0" text="cena" />
                        <TableColumn fx:id="DISCOUNT" prefWidth="90.0" text="zniżka" />
                        <TableColumn fx:id="AMOUNT" prefWidth="75.0" text="Column X" />
                    </columns>
                  </TableView>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
